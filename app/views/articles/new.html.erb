<%= form_for :article, url: articles_path do |f| %>
  <p>
    <%= f.label :rubrik %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>

  <p>
    <%= f.label :namn %><br>
    <%= f.text_field :author %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>